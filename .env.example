# PostgreSQL Configuration
POSTGRES_DB=ecommerce
POSTGRES_USER=ecomuser
POSTGRES_PASSWORD=changeme

# Spring Boot Configuration
SPRING_DATASOURCE_URL=jdbc:postgresql://db:5432/ecommerce
SPRING_DATASOURCE_USERNAME=ecomuser
SPRING_DATASOURCE_PASSWORD=changeme
SPRING_PROFILES_ACTIVE=prod

# Docker Hub Configuration (for CI/CD)
DOCKERHUB_USERNAME=your_dockerhub_username
DOCKERHUB_TOKEN=your_dockerhub_token

# Docker Image Tags
DOCKER_IMAGE_BACKEND=your_dockerhub_username/ecom-backend
DOCKER_IMAGE_FRONTEND=your_dockerhub_username/ecom-frontend

# Application Configuration
SERVER_PORT=5000
VITE_BACK_END_URL=http://localhost:5000

# JWT Configuration
SPRING_APP_JWTSECRET=your_jwt_secret_key_here_min_50_chars_recommended
SPRING_APP_JWTEXPIRATIONMS=6840000
